{
  "name": "mixserver-docs",
  "private": true,
  "scripts": {
    "docs:dev": "vuepress dev . --config config.mjs",
    "docs:build": "vuepress build . --config config.mjs",
    "docs:clean": "node -e \"const fs=require('fs');const path=require('path');['temp','cache','.vuepress'].forEach(d=>{const p=path.join(process.cwd(),d);if(fs.existsSync(p))fs.rmSync(p,{recursive:true,force:true});});\""
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-rc.26",
    "@vuepress/plugin-markdown-chart": "2.0.0-rc.121",
    "@vuepress/theme-default": "2.0.0-rc.121",
    "mermaid": "^11.12.0",
    "sass-embedded": "^1.77.5",
    "vuepress": "2.0.0-rc.26"
  }
}
